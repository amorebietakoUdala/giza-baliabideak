{% extends 'base.html.twig' %}

{% block title %}{{ 'app.title.subApplication.index' | trans }}{% endblock %}

{% block body %}
	<main class="container-fluid">
        <div id="content" class="container-fluid" {{ stimulus_controller('table', 
                    { 'url': path('subApplication_index'),
                    'exportName': 'subApplications',
                })}} data-action="entity:success->table#refreshContent" data-table-target="content">
                {{ include('sub-application/_list.html.twig', { 
                    'subApplications': subApplications,
                    'pageSize' : pageSize,
                    'page': page,
                    'sortName': sortName,
                    'sortOrder': sortOrder,
                }) }}
        </div>
	</main>
{% endblock %}